    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "rollup": "2.66.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.27.1",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
